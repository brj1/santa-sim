<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="santa.xsd">
	<replicates>10</replicates>

	<simulation>
		<genome>
			<length>2697</length>
			<sequences>
				
			</sequences>
			<feature>
				<name>CDS</name>
				<type>aminoAcid</type>
				<coordinates>1-2697</coordinates>
			</feature>
		</genome>

		<population>
			<populationSize>1</populationSize>

			<inoculum>all</inoculum>
		</population>
		
<populationType>dynamicPopulation</populationType>

<growthModel>
<growthRate>1</growthRate>
<carryingPopulation>1000</carryingPopulation>
</growthModel>

		<fitnessFunction>
            <neutralFitness>
	            <feature>CDS</feature>
	            <sites>1-2697</sites>
            </neutralFitness>
        </fitnessFunction>

		<mutator>
			<nucleotideMutator>
				<mutationRate>1.0E-6</mutationRate>
				<transitionBias>2.0</transitionBias>
			</nucleotideMutator>
		</mutator>

		<replicator>
			<clonalReplicator />
		</replicator>

		<epoch>
			<generationCount>100</generationCount>
		</epoch>

<samplingSchedule>

    
    <sampler>
        <atFrequency>1</atFrequency>
        <fileName>1_1000_100_%r.csv</fileName>
        <statistics />
    </sampler>

</samplingSchedule>

	</simulation>

</santa>
